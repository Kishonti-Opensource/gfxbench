<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_enabled="true">
		<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
			<solid android:color="@color/editbox_background" />
			<stroke android:width="1dp" android:color="@color/editbox_border" />
		</shape>
	</item>
	<item>
		<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
			<solid android:color="@color/editbox_disabled_background" />
			<stroke android:width="1dp" android:color="@color/editbox_disabled_border" />
		</shape>
	</item>
</selector>
